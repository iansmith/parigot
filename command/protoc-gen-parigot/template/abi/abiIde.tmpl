//+build !parigot_abi

// This file is here to provide your IDE with code that it thinks is what is being
// called when you reference an abi function.  This is not at all what's going to
// happen in practice, but it is nice to have api documentation and such in your
// IDE.

package abi

{{- range .abi.GetMethod}}
{{- $inEmpty:=isInputEmpty .}}
{{- $outEmpty:=isOutputEmpty .}}
    {{- if $inEmpty }}
        {{- if $outEmpty }}
    func {{.GetName}}() {}
        {{- else}}
    func {{.GetName}}() {{outputTypeName .}} {
        {{outputZeroDefaultReturn .}}
    }
        {{- end}}
    {{- else}}
        {{- if $outEmpty }}
    func {{.GetName}}({{inputParamNameSet true false .}}) {}
        {{- else}}
    func {{.GetName}}({{inputParamNameSet true false .}}) {{outputTypeName .}} {
        {{outputZeroDefaultReturn .}}
    }
        {{- end -}}
    {{- end}}
{{end}}

